<h1>Welcome <%= current_user.username %>!</h1>

<b>Your Feed!</b>
<div class="infinite-tweets">
  <ul id="feed">
  </ul>
  <a href="javascript:void(0)" class="fetch-more">Fetch Tweets</a>

  <script id="feed-script" type="text/template">
    <%% tweets.forEach(function (tweet) { %>
      <li><%%= tweet.content %> by <%%= tweet.user.username %></li>
    <%% }); %>
  </script>
</div>

<%= render "tweets/form" %>
